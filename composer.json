{
	"name" : "sweelix/yii2-nosql",
	"description" : "PHP 5.4+ nosql driver for yii2",
	"type" : "yii2-extension",
	"authors" : [{
			"name" : "Philippe Gaultier",
			"email" : "pgaultier@sweelix.net"
		}, {
			"name" : "Christophe Latour",
			"email" : "clatour@ibitux.net"
		}
	],
	"keywords" : [
		"yii2",
		"riak",
		"nosql",
		"riakcs"
	],
	"license" : [
		"BSD-3-Clause"
	],
	"require" : {
		"ext-curl" : "*",
		"php" : ">=5.4.0",
		"sweelix/curl" : "dev-devel"
	},
	"require-dev" : {
		"phpunit/phpunit" : "3.7.*",
		"phpunit/php-invoker" : ">=1.1",
		"yiisoft/yii2" : "*"
	},
	"autoload" : {
		"psr-4" : {
			"sweelix\\yii2\\nosql\\" : "src"
		}
	},
	"minimum-stability" : "dev",
	"config" : {
		"vendor-dir" : "vendor"
	}
}